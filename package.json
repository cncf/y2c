{
  "name": "y2c",
  "version": "1.0.0",
  "private": true,
  "description": "A Probot app",
  "author": "Jordi Noguera <jordinoguera83+github@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/cncf/y2c.git",
  "homepage": "https://github.com/cncf/y2c",
  "bugs": "https://github.com/cncf/y2c/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "concurrently --kill-others \"nodemon\" \"yarn run webpack\"",
    "webpack": "cd frontend && yarn start",
    "start": "probot run ./lib/app.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@octokit/auth-oauth-app": "^2.2.1",
    "googleapis": "^46.0.0",
    "probot": "^9.5.3"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "jest": "^24.9.0",
    "nock": "^11.4.0",
    "nodemon": "^2.0.0",
    "smee-client": "^1.1.0",
    "standard": "^14.3.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": [
      "/frontend/"
    ]
  },
  "nodemonConfig": {
    "exec": "yarn start",
    "ignore": ["node_modules/", "frontend/"]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
