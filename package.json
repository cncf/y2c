{
  "name": "y2c",
  "version": "1.0.0",
  "private": true,
  "description": "A Probot app",
  "author": "Jordi Noguera <jordinoguera83+github@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/cncf/y2c.git",
  "homepage": "https://github.com/cncf/y2c",
  "bugs": "https://github.com/cncf/y2c/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "concurrently --kill-others \"yarn run datastore:start\" \"nodemon\" \"sleep 5 && yarn run client:dev\"",
    "client:dev": "./bin/set-client-env && cd client && yarn start",
    "client:build": "./bin/set-client-env && cd client && yarn install && yarn run build",
    "datastore:start": "gcloud beta emulators datastore start --host-port=0.0.0.0:8081",
    "start": "NODE_PATH=./src probot run ./src/app.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "gcp-build": "./bin/gcp-build",
    "deploy": "gcloud app deploy appengine.yml --quiet"
  },
  "dependencies": {
    "@google-cloud/datastore": "^5.0.3",
    "@octokit/auth-oauth-app": "^2.2.1",
    "googleapis": "^46.0.0",
    "probot": "^9.5.3"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "jest": "^24.9.0",
    "nock": "^11.4.0",
    "nodemon": "^2.0.0",
    "smee-client": "^1.1.0",
    "standard": "^14.3.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": [
      "/client/"
    ]
  },
  "nodemonConfig": {
    "exec": "yarn start",
    "ignore": [
      "node_modules/",
      "client/"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "exports": {}
}
